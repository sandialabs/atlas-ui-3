{
  "atlas_rag": {
    "type": "http",
    "display_name": "ATLAS RAG",
    "description": "External ATLAS RAG API for document retrieval and question answering.",
    "icon": "database",
    "url": "${ATLAS_RAG_URL}",
    "bearer_token": "${ATLAS_RAG_BEARER_TOKEN}",
    "default_model": "openai/gpt-oss-120b",
    "top_k": 4,
    "timeout": 60.0,
    "groups": [
      "users"
    ],
    "compliance_level": "Internal"
  },
  "corporate_cars": {
    "type": "mcp",
    "display_name": "Corporate Cars",
    "description": "Fleet RAG server: discover resources and retrieve locations/meta-data for corporate cars in use by employees.",
    "icon": "car",
    "command": [
      "python",
      "mcp/corporate_cars/main.py"
    ],
    "cwd": "atlas",
    "groups": [
      "users"
    ],
    "compliance_level": "SOC2"
  }
}
